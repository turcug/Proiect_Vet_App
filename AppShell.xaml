<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Proiect_Vet_App.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Proiect_Vet_App"
    Shell.FlyoutBehavior="Disabled"
    Title="Proiect_Vet_App">

    <TabBar>
        <ShellContent Title="Carnet de Sanatate" ContentTemplate="{DataTemplate local:MainPage}" />
        <ShellContent Title="Cabinete Veterinare" ContentTemplate="{DataTemplate local:Adaugare_Cabinete}" />
        <ShellContent Title="About" ContentTemplate="{DataTemplate local:AboutPage}" />
    </TabBar>
</Shell>
